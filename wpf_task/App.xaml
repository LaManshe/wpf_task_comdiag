<Application x:Class="wpf_task.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:wpf_task"
             xmlns:com="clr-namespace:wpf_task.Commands"
             xmlns:conv="clr-namespace:wpf_task.Converters"
             xmlns:vm="clr-namespace:wpf_task.ViewModels"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator"/>
            <com:DialogResultButtonsCommand x:Key="DialogResultButtonsCommand"/>
            <conv:Base64ToImageConverter x:Key="Base64ToImageConverter"/>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
